{
  "name": "environments",
  "displayName": "environments",
  "description": "",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "browser": "./dist/extension.js",
  "contributes": {
    "viewsWelcome": [
      {
        "view": "environments-sidebar-view",
        "contents": "No .env files found."
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "environments-sidebar-view",
          "title": "Environments",
          "icon": "assets/icon.svg"
        }
      ]
    },
    "views": {
      "environments-sidebar-view": [
        {
          "type": "tree",
          "id": "environments-sidebar",
          "name": "Environments",
          "icon": "media/icon.svg",
          "contextualTitle": "Environments"
        }
      ]
    }
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --mode development --watch",
    "package-web": "webpack --mode production",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^10.0.7",
